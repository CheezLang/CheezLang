_ :: Main()
Main :: () {
    arr : Bar
    baz : Baz

    baz.a = 5
    baz.b = "hi"
}

sub :: (a: int, b: int) -> int {
    return a - b
}

Bar :: Array[int, Add[5, 6]]

Add :: (A: int, B: int) const A + B
Array :: (T: type, Size: int) struct {
    arr : [sub(Size, 2)]T
}

Foo :: (Body: string) struct {
    Body
}

Baz :: Foo["a: int
b: string"]
