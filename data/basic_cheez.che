
foo :: () {
    T1 :: fib(N)
    print_string("foo")
    bar()
}

bar :: () {
    T2 :: fib(N + 1)
    print_string("bar")
}

Main :: () {
    T3 :: fib(N - 1)
    print_string("Main")
    foo()
}

fib :: (n: int) -> int {
    if n <= 1 {
        return 1
    }

    return fib(n - 1) + fib(n - 2)
}

N :: 20