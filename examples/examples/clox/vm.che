#load("object")
#load("compiler")

InterpretResult :: enum {
    Ok
    CompileError
    RuntimeError
}

fn run() -> InterpretResult {
    return InterpretResult.RuntimeError
}

fn interpret(source: string) -> InterpretResult {
    function := compile(source)
    if function == null {
        return InterpretResult.CompileError
    }

    println("ok")
    return run()
}

