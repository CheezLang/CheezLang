
enum ObjKind {
    BoundMethod
    Class
    Closure
    Function
    Instance
    Native
    String
    UpValue
}

struct Obj {
    kind    : ObjKind
    is_dark : bool
    next    : &Obj
}

struct ObjFunction {
    obj     : Obj   = Obj(kind = ObjKind.Function)
    arity   : int
    chunk   : Chunk
    name    : ObjString
}

struct ObjString {
    obj     : Obj   = Obj(kind = ObjKind.String)
    chars   : []char
    hash    : u32
}