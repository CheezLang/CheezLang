
ObjKind :: enum {
    BoundMethod
    Class
    Closure
    Function
    Instance
    Native
    String
    UpValue
}

Obj :: struct {
    kind    : ObjKind
    is_dark : bool
    next    : &Obj
}

ObjFunction :: struct {
    obj     : Obj   = Obj(kind = ObjKind.Function)
    arity   : int
    chunk   : Chunk
    name    : ObjString
}

ObjString :: struct {
    obj     : Obj   = Obj(kind = ObjKind.String)
    chars   : []char
    hash    : u32
}