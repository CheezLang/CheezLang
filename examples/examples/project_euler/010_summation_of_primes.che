#test_expect_output("142913828922")

#load("std:io/io")
#load("std:math")

is_prime :: (i: int) -> bool {
    if i < 2 {
        return false
    }
    for(inclusive=true) 2..int(sqrt(double(i))) {
    // for(inclusive=true) : 2..int(i/2) {
        if i % it == 0 {
            return false
        }
    }

    return true
}

Main :: () {
    sum := 0
    for 0..2000000 {
        if is_prime(it) {
            sum += it
        }
    }
    println(sum)
}