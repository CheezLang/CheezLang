// https://projecteuler.net/problem=3

#test_expect_output("6857")

#load("std:io/io")

Main :: () {
    n := 600851475143

    for(inclusive=true) :2..n/2 {
        while n % it == 0 {
            n /= it
        }
        
        if n == 1 {
            println(it)
            break
        }
    }
}