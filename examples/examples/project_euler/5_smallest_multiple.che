// https://projecteuler.net/problem=5

#test_expect_output("232792560")
#load("std:io/io")

fn Main() {
    let start = 2 * 3 * 5 * 7 * 11 * 13 * 17 * 19

    while let n = start, true, n += 1 #label outer {
        // test if n is divisible by 2..20
        for(inclusive=true) : 2..20 {
            if n % it != 0 then continue outer
        }

        println(n)
        break
    }
}