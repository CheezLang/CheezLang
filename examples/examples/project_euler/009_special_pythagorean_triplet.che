// https://projecteuler.net/problem=9

#test_expect_output("31875000")

io :: import std.io
use import std.math

Main :: () {
    for a : 1..500 #label outer {
        for b : a+1..500 {
            c_squared := a * a + b * b
            c := int(sqrt(double(c_squared)))

            if c * c != c_squared then continue
            if a + b + c == 1000 {
                io.println(a * b * c)
                break outer
            }
        }
    }
}