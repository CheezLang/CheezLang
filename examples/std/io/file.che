#load("std:os/windows")

fn loadFile(filename: c_string, str: String&) -> bool {
    let of: _OFSTRUCT
    let handle = OpenFile(filename, &of, 0)
    if handle == -1 {
        return false
    }
    let size = GetFileSize((HANDLE)handle, null)

    str.resize((i32)size)
    
    let bytesRead = 0
    ReadFile((HANDLE)handle, str.data, size, &bytesRead, null)
    CloseHandle((HANDLE)handle)

    return true
}

