#load("allocator.che")
#load("memory.che")

struct StdHeapAllocator {
    _unused: int
}

impl Allocator for StdHeapAllocator {
    fn allocate(count: ulong, elementSize: ulong) -> any& {
        return malloc((int)(count * elementSize))
    }

    fn reallocate(mem: any&, count: ulong, elementSize: ulong) -> any& {
        return realloc(mem, (int)(count * elementSize))
    }

    fn free(mem: any&) {
        free(mem)
    }
}
