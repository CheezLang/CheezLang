#load("../string.che")
#load("token.che")

struct Lexer {
    text: String
    location: TokenLocation
}

impl Lexer {
    ref fn Init(filename: string) -> bool {
        text = NewString()
        if LoadFile(filename, &text) == false { return false }

        location = new TokenLocation {
            filename, 1, 0, 0
        }

        return true
    }

    ref fn Cleanup() {
        text.Dispose()
    }

    ref fn NextToken() {
        // let token = new Token {

