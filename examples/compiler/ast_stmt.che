

#load("ast_expr")

Stmt :: struct {
    concrete    : StmtType
}

StmtType :: enum {
    FunctionDecl    : FunctionDecl
}

impl Printable for Stmt {
    print :: (ref Self, str: ref String, format: string) {
        use StmtType
        match concrete {
            FunctionDecl($func) -> {
                str.appendf("fn {}() ``{``}", func.name)
            }
        }
    }
}

FunctionDecl :: struct {
    name    : string
}
