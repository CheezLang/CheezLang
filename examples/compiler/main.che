#load("../stdio.che")
#load("../string.che")
#load("../file.che")
#load("lexer.che")
#load("token.che")

fn RunCompiler(filename: string) {
    let lexer: Lexer
    lexer.Init(filename)

    let cond = true
    while cond {
        let t = lexer.NextToken()

        t.Print()
        Println("")

        if t.type == TokenType.EOF {
            cond = false
        }
    }

    Println("`ndone")

    lexer.Cleanup()
}
