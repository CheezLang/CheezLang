#test_expect_output("5
hello
true")


#load("std:c")
#load("std:alloca")
// #load("std:string")

Printable :: trait {
    print :: (ref Self);
}

impl Printable for int {
    print :: (ref Self) {
        c_printf("%lld", self)
    }
}

impl Printable for bool {
    print :: (ref Self) {
        c_printf("%s", if self {"true"c} else {"false"c})
    }
}

impl Printable for string {
    print :: (ref Self) {
        c_printf("%.*s", self.length, self.data)
    }
}

println :: (p: Printable) {
    p.print()
    c_puts("")
}

Main :: () {
    five := 5
    hello := "hello"
    tru := true
    println(five)
    println(hello)
    println(tru)
}
