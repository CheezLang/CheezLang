#load("std:preload")

c_printf :: (format: c_string) #varargs #linkname("printf");

Main :: () {
    test1(5)
    test2(6)
    test3(4)
    test4(10)
    test5(15)
    fizz_buzz(20)
}

fizz_buzz :: (n: int) {
    for a : 1..n {
        if a % 5 == 0 and a % 3 == 0 {
            c_printf("FizzBuzz ")
        } else if a % 5 == 0 {
            c_printf("Buzz ")
        } else if a % 3 == 0 {
            c_printf("Fizz ")
        } else {
            c_printf("%lld ", a)
        }
    }
}

test5 :: (n: int) {
    while true, n = n - 1 {
        if n == 0 {
            break
        }

        if n % 3 == 0 {
            continue
        }

        c_printf("%lld,", n)
    }
    c_printf("`n")
}

test4 :: (n: int) {
    for a : 0..n {
        if a % 2 == 0 {
            continue
        }

        c_printf("%lld", a)
    }
    c_printf("`n")
}

test3 :: (n: int) {
    while a := 0, true, a = a + 1 {
        if a == n {
            break
        }

        c_printf("%lld", a)
    }
    c_printf("`n")
}

test2 :: (n: int) {
    for a : 0..n {
        c_printf("%lld", a)
    }
    c_printf("`n")
}

test1 :: (n: int) {
    a := 0
    while a < n {
        c_printf("%lld", a)
        a = a + 1
    }
    c_printf("`n")
}

