#test_expect_output("0, 1, 2, 3")

fn c_printf(format: c_string) #varargs #linkname("printf");

fn Main() {
    c_printf("%d, %d, %d, %d", foo(int), bar(1), baz(int, 2), uiae(3, int))
}

fn foo($T: type) -> T {
    return 0
}

fn bar(x: $T) -> T {
    return x
}

fn baz($T: type, x: $T2) -> T {
    return x
}

fn uiae($X: int, $T: type) -> T {
    return X
}
