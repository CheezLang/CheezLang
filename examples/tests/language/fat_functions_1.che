#test_expect_output("
foo()
test1()
foo(): ...

baz()
test2() -> 3
baz(): 3

bar(9)
test3(9) -> 14
bar(9): 14
")

#load("std:io/io")

foo :: (f: Fn()) {
    println("foo()")
    f()
    println("foo(): ...")
}

baz :: (f: Fn() -> int) {
    println("baz()")
    result := f()
    printfln("baz(): {}", result)
}

bar :: (f: Fn(int) -> int, x: int) {
    printfln("bar({})", x)
    result := f(x)
    printfln("bar({}): {}", (x, result))
}

test1 :: () {
    println("test1()")
}

test2 :: () -> int {
    println("test2() -> 3")
    return 3
}

test3 :: (a: int) -> int {
    result := a + 5
    printfln("test3({}) -> {}", (a, result))
    return result
}

Main :: () {
    println("")

    {
        f : Fn() = cast test1
        foo(f)
        println("")
    }

    {
        f : Fn() -> int = cast test2
        baz(f)
        println("")
    }

    {
        f : Fn(int) -> int = cast test3
        bar(f, 9)
        println("")
    }
}