#test_expect_output("5, 6, 5, 6, 5, 6")
#test_expect_output("7, 8, 7, 8, 7, 8")

#load("std:preload")

// required for printing
fn c_printf(format: c_string) #varargs #linkname("printf");

// tuples and order
let a       : (int, int) = (5, 6)
let b       : (int, int) = a
let (x, y)  : (int, int) = b

let (x2, y2)    : (int, int) = b2
let b2          : (int, int) = a2
let a2          : (int, int) = (7, 8)

fn Main() {
    c_printf("%d, %d, %d, %d, %d, %d`n", a[0], a[1], b[0], b[1], x, y)
    c_printf("%d, %d, %d, %d, %d, %d`n", a2[0], a2[1], b2[0], b2[1], x2, y2)
}
