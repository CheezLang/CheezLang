#test_expect_output("5, 6, 5, 6, 5, 6")
#test_expect_output("7, 8, 7, 8, 7, 8")

C :: import std.c

// tuples and order
a : (int, int) = (5, 6)
b : (int, int) = a
(x, y) : (int, int) = b

(x2, y2) : (int, int) = b2
b2 : (int, int) = a2
a2 : (int, int) = (7, 8)

Main :: () {
    C.printf("%d, %d, %d, %d, %d, %d`n", a[0], a[1], b[0], b[1], x, y)
    C.printf("%d, %d, %d, %d, %d, %d`n", a2[0], a2[1], b2[0], b2[1], x2, y2)
}
