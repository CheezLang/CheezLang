#test_expect_output("0")
#test_expect_output("3")
#test_expect_output("6")
#test_expect_output("6.000000")

C :: import std.c

Test :: struct {
    x: int
}

impl Test {
    foo :: (ref Self, a: $T) {
        self.x = cast a
    }

    bar :: (Self, $T: type) -> T {
        return cast self.x
    }
}

Main :: () {
    t := Test(0)
    C.printf("%lld`n", t.x)
    t.foo(3)
    C.printf("%lld`n", t.x)
    t.foo(6)
    C.printf("%lld`n", t.x)
    d := t.bar(double)
    C.printf("%f`n", d)
}