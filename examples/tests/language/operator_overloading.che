#load("std:preload")
#load("std:math")

fn c_printf(format: c_string) #varargs #linkname("printf");

struct Num #copy {
    pub x: int
}

fn num_add(a: Num, b: Num) -> Num #operator("+") {
    return Num(a.x + b.x)
}

fn num_inc(a: &Num, b: Num) #operator("+=") {
    a.x += b.x
}

typedef Vec2 = Vector2[double]

fn Main() {
    {
        let a = Num(1)
        let b = Num(2)
        let c = a + b

        c_printf("%lld`n", a.x)
        c_printf("%lld`n", b.x)
        c_printf("%lld`n", c.x)

        (a, c) += (b, Num(5))
        
        c_printf("%lld`n", a.x)
        c_printf("%lld`n", b.x)
        c_printf("%lld`n", c.x)
        c_printf("`n")
    }

    {
        let a = Vec2(1, 2)
        let b = Vec2(3, 4)
        let c = a + b

        let d = Vec2(5, 6)
        d += c

        c_printf("%f, %f`n", a.x, a.y)
        c_printf("%f, %f`n", b.x, b.y)
        c_printf("%f, %f`n", c.x, c.y)
        c_printf("%f, %f`n", d.x, d.y)

        
        c_printf("`n")

        a -= Vec2(0.5, 1)
        c_printf("%f, %f`n", a.x, a.y)

        a *= 4.0
        c_printf("%f, %f`n", a.x, a.y)

        a /= Vec2(4, 2)
        c_printf("%f, %f`n", a.x, a.y)

        a *= Vec2(3, 2)
        c_printf("%f, %f`n", a.x, a.y)

        a = -a
        c_printf("%f, %f`n", a.x, a.y)
    }
}