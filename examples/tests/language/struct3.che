#test_expect_output("2")
#test_expect_output("3.600000")
#test_expect_output("5")
#test_expect_output("3.600000")
#test_expect_output("5")

C :: import std.c

Test :: struct(T: type) {
    x: T
}

impl(T: type) Test[T] {
    foo :: (&Self, a: int) -> Test[T] {
        self.x = cast a
        return Test[T](cast a)
    }
}

Main :: () {
    t := Test[int](2)
    t2 := Test[double](3.6)

    C.printf("%lld`n", t.x)
    C.printf("%f`n", t2.x)

    uiae := t.foo(5)

    C.printf("%lld`n", t.x)
    C.printf("%f`n", t2.x)
    C.printf("%lld`n", uiae.x)
}