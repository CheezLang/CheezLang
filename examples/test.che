#load("test2")
#load("std:util")

fn foo() -> int {
    let a = getCurrentTimeInMilliseconds()
    let b = getCurrentTimeInMilliseconds()
    return b - a
}

fn Main() {
    let t1 = getCurrentTimeInMilliseconds()

    let result = 0
    while let a = 0; a < 10000000; a += 1 {
        let x = foo()
        let y = foo()
        result += x + y
    }

    let t2 = getCurrentTimeInMilliseconds()
    c_printf("%lld`n", t2 - t1)
    c_printf("%lld`n", result)
}