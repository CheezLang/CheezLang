struct Vec2 {
    x: float
    y: float
}

impl Vec2 {
    fn add(Self, other: Self) -> Self {
        return new Vec2 {
            self.x + other.x,
            self.y + other.y
        }
    }
    
    fn add_to_this(ref Self, other: Self) {
        self.x += other.x
        self.y += other.y
    }

    fn create(x: float = 0, y: float = 0) -> Self {
        return new {x, y}
    }
}

fn Main() {
    let v1 = Vec2::create(1, 2)
    let v2 = Vec2::create(3, 4)

    let v3 = v1.add(v2)

    v3.add(Vec2::create(5, 6))
}