#load("std/string.che")
#load("std/mem/memory.che")
#load("std/io/io.che")

struct Vec3 {
    x: int
    y: int
    z: int
}

impl Printable for Vec3 {
    fn Print(str: String&, format: char[]) {
        Sprintf(str, "({}, {}, {})", [x, y, z])
    }
}

fn Main()
{
    Printf("Hi {0}, my name is {1}. How are you doing, {0}? - {2}", ["friend", "me", new Vec3{147, 258, 369}])
}
