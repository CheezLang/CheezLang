struct Vec3 {
    x: float
    y: float
    z: float
}

impl Vec3 {
    ref fn add(other: Vec3) -> Vec3 {
        return new Vec3 {
            x = self.x + other.x
            y = self.y + other.y
            z = self.z + other.z
        }
    }
}

impl Printable for Vec3 {
    fn print(str: String&, format: string) {
        sprint_f(str, "({}, {}, {})", [self.x, self.y, self.z])
    }
}

fn Main() {
    let a = new Vec3 { 1, 2, 3 }
    let b = new Vec3 { x = 4, y = 5, z = 6 }

    let c = a.add(b)

    print_f("
  {}
+ {}
  ------------------------------
= {}", [a, b, c])

    // trait_function_call_performance()
