// #load("tests/language/local_variables.che")
#load("test2")
// #load("std:math")

struct Test {
    a: int
}

impl Test {
    fn foo(self) {
        self.a += 1
        a += 1
    }

    fn bar(*self) {
        self.a += 1
        a += 1
    }
}

fn Main() {
    let t = new Test { 0 }

    c_printf("%lld`n", t.a)
    t.foo()
    c_printf("%lld`n", t.a)
    t.bar()
    c_printf("%lld`n", t.a)
}