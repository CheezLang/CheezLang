fn Main() {
    let a: Vec2
    let b: Vec2

    a.x = 1
    a.y = 2

    b.x = 3
    b.y = 4

    let c = a::Add(b)

    a::Print()
    b::Print()
    c::Print()
}

struct Vec2 {
    x: f32
    y: f32
}

impl Vec2 {
    fn Add(v: Vec2): Vec2 {
        let result: Vec2
        result.x = x + v.x
        result.y = y + v.y
        return result
    }

    fn Print() {
        println "Vec2(", x, ", ", y, ")"
    }
}
