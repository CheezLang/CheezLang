fn Main()
{
    let args: any[] = Array(2)
    args[0] = (byte)123
    args[1] = (short)456
    args[2] = "hi wie gehts?"

    Printlnfn("args[0]: {i8}`nargs[1]: {i16}`nargs[2]: {s}", args)
