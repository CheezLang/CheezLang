fn Main() {
    let a: Vec2
    a.x = 1
    a.y = 2

    a::Print()
    a::Print()

    println ""

    (&a)::Print()
    (&a)::Print()
}

struct Vec2 {
    x: f32
    y: f32
}

impl Vec2 {
    fn Print() {
        x = x + 10
        println "Vec2(", x, ", ", y, ")"
    }
}

impl Vec2* {
    fn Print() {
        x = x + 10
        println "Vec2(", x, ", ", y, ")"
    }
}
